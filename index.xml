<body>
<script src="https://hangoutsapi.talkgadget.google.com/hangouts/api/hangout.js?v=1.3" ></script>
<div id="diceui" style="visibility: hidden">
<h3>Roll The Dice</h3>
<div class="tagline"><i>By <a href="https://plus.google.com/111734237778995429762/posts" target="_new">Richard Wooding</a></i></div>
<div id="primaryUI">
No. of die to roll <select id="numDice">
<option>1</option>
<option>2</option>
<option>3</option>
<option>4</option>
<option>5</option>
<option>6</option>
<option>7</option>
<option>8</option>
<option>9</option>
<option>10</option>
</select><input type="button" class="button" onClick="rollNumDice()" value="Roll!"><input type="button" class="button" onClick="clearDice()" value="Clear!"></input><br/>
Style <select id="variant">
<option value="western6">Western 6-sided</option>
<option value="asia6">Asia 6-sided</option>
<option value="fudge6">FUDGE 6-sided</option>
<option value="fate6">FATE 6-sided</option>
<option value="d20">20-sided (beta)</option>
</select>
</div>
<div id="preview">
</div>
<div id="comingSoon" class="tagline">
Coming soon
<ul>
   <li>More dice styles</li>
   <li>Translation into more languages</li>
</ul>
</div>
<div style="display: none">
Help with additional dice tiles
<ul>
  <li>Can be any number of sides</li>
  <li>Format should be PNG</li>
  <li>Size should be 119x119 or small variation</li>
  <li>Colour, transperancy, and alpha-transparency is supported</li>
  <li>E-mail to <a href="mailto:rollthedice@richardwooding.com">rollthedice@richardwooding.com</a></li>
</ul>
</div>
<div style="margin-left: 8x; margin-right: 8px">
<p>This extension was dreamed up in:<br/>
<a href="https://plus.google.com/118252934082486983748/posts" target="_new">The Pirate's Pub</a></p>
<div>
</div>
<script>
var sides = [
  "data:image/png;base64,",
  "data:image/png;base64,",
  "data:image/png;base64,",
  "data:image/png;base64,",
  "data:image/png;base64,",
  "data:image/png;base64,"
];
var asia = [
  "data:image/png;base64,",
  "data:image/png;base64,",
  "data:image/png;base64,",
  "data:image/png;base64,",
  "data:image/png;base64,",
  "data:image/png;base64,"
];

var fudge6 = [
  "data:image/png;base64,",
  "data:image/png;base64,",
  "data:image/png;base64,",
  "data:image/png;base64,",
  "data:image/png;base64,",
  "data:image/png;base64,"
];

var fate6 = [
  "http://dice.richardwooding.com/Dice_Fate_Minus.png",
  "http://dice.richardwooding.com/Dice_Fate_Minus.png",
  "http://dice.richardwooding.com/Dice_Fate_Plus.png",
  "http://dice.richardwooding.com/Dice_Fate_Plus.png",
  "http://dice.richardwooding.com/Dice_Fate_Blank.png",
  "http://dice.richardwooding.com/Dice_Fate_Blank.png"
];

var d20 = new Array();
for (var i=1; i<=20; i++) {
  d20.push("http://dice.richardwooding.com/20large/" + i + ".png?id=5");
}

var dice = {"western6" : sides, "asia6" : asia, "fudge6" : fudge6, "fate6" : fate6, "d20" : d20};
var settings = { "western6" : { "magnitude" : 0.1, "x":-0.45,"y":0.4,"xs":0.1}, 
  "asia6" : { "magnitude" : 0.1, "x":-0.45, "y":0.4,"xs":0.1}, 
  "fudge6" : { "magnitude" : 0.1, "x":-0.45, "y":0.4,"xs":0.1 }, 
  "fate6" : {"magnitude" : 0.1, "x":-0.45, "y":0.4,"xs":0.1 },
  "d20" : { "magnitude" : 0.25, "x":-0.3, "y":0.3,"xs":0.2 }};

var diceOverlays = new Array();
function rollNumDice() {
  var numDiceSel = document.getElementById("numDice");
  var varSel = document.getElementById("variant");
  rollDice(numDiceSel.options[numDiceSel.selectedIndex].value, varSel.options[varSel.selectedIndex].value);
}
function rollDice(times,type) {
    clearDice();
    var previewHTML = "";
    for (var i=0; i < times; i++) {
      var side=Math.floor(Math.random()*dice[type].length);
      var sideImg = gapi.hangout.av.effects.createImageResource(dice[type][side]);
      previewHTML += "<img src='" + dice[type][side] + "' width='60' height='60'/>"
      var s = settings[type];
      diceOverlay = sideImg.createOverlay({'position': {'x':s.x+(s.xs*i), 'y':s.y}, 'scale' : {'magnitude': s.magnitude, 'reference' : gapi.hangout.av.effects.ScaleReference.HEIGHT}});
      diceOverlays.push(diceOverlay);
      diceOverlay.setVisible(true);
    }
    document.getElementById("preview").innerHTML = previewHTML;
}

function clearDice() {
    document.getElementById("preview").innerHTML="";  
    diceOverlays.forEach(function(diceOverlay) {
        diceOverlay.setVisible(false);
        diceOverlay.dispose();
    });
    diceOverlays = new Array();
}

function init() {
  // When API is ready...
  gapi.hangout.onApiReady.add(
      function(eventObj) {
        if (eventObj.isApiReady) {
          document.getElementById('diceui')
            .style.visibility = 'visible';
        }
      });
}

// Wait for gadget to load.
gadgets.util.registerOnLoadHandler(init);
</script>
</body>
</html>
